            "name": "solver.cpp",
            "contents": "\n#include <iostream>\n#include <sstream>\n#include \"lib.hpp\"\n\nusing namespace std;\n\nint main(){\n    string line;\n    getline(cin, line);\n    BTree bt(line);\n    bt.show_in_order();\n}\n\nBTree::~BTree(){\n    destroy(this->root);\n}\n\nvoid BTree::show_in_order(){\n    cout << \"[ \";\n    show_in_order(root);\n    cout << \"]\\n\";\n}\n\nvoid BTree::destroy(Node * node){\n    if(node == nullptr)\n        return;\n    destroy(node->left);\n    destroy(node->right);\n    delete node;\n}\n    \n\nvoid BTree::show_in_order(Node * node){\n    if(node == nullptr)\n        return;\n    show_in_order(node->left);\n    cout << node->value << \" \";\n    show_in_order(node->right);\n}\n",
